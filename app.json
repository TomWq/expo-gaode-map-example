{
  "expo": {
    "name": "expo-gaode-map-example",
    "slug": "expo-gaode-map-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expogaodemapexample",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo-gaode-map-example",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.example.expogaodemapexample"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-gaode-map",
        {
          "iosApiKey": "9f09c08f902c69919c3819cdd30f0179",
          "androidApiKey": "c9d3831e42431d68442e279051999bef",
          "enableLocation": true,
          "locationDescription": "我们需要访问您的位置信息以提供地图服务",
          "enableBackgroundLocation": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
